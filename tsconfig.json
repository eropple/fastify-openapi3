{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
      "rootDir": "./src",
      "outDir": "./dist",
      "target": "ES2017",
      "lib": [
        "ESNext",
      ],
      "moduleResolution": "node",
      "paths": {
        "@/*": ["./src/*"],
      },
      "module": "esnext",
    },
    "include": [
      "./src/*.ts",
      "./src/**/*.ts"
, "test-manual/rapidoc.test.ts"    ],
    "typedocOptions": {
      "entryPoints": [
        "src/plugin.ts",
        "src/options.ts",
        "src/operation-helpers.ts",
      ],
      "entryPointStrategy": "expand",
      "out": "docs"
    }
  }
