    "utility-types": "^3.11.0"
  },
  "peerDependencies": {
    "@scalar/fastify-api-reference": "^1.25.7",
    "ajv": "^8",
    "fastify": "^5",
    "typebox": "^1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@fastify/cookie": "^11.0.2",
    "@fastify/formbody": "^8.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.12",
