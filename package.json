  },
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "*",
    "src/**/*",
    "test/**/*",
    "dist/**/*",
    "examples/**/*"
  ],
  "scripts": {
